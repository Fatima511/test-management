<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_sequence_test_case" model="ir.sequence">
            <field name="name">Test Case</field>
            <field name="code">test.case</field>
            <field name="prefix">TC</field>
            <field name="padding">5</field>
        </record>

         <record id="ir_sequence_test_run" model="ir.sequence">
            <field name="name">Test Run</field>
            <field name="code">test.run</field>
            <field name="prefix">RUN</field>
            <field name="padding">7</field>
        </record>

        <record id="ir_sequence_test_bug" model="ir.sequence">
            <field name="name">Test Bug</field>
            <field name="code">test.bug</field>
            <field name="prefix">BUG</field>
            <field name="padding">7</field>
        </record>

    </data>
</odoo>
